{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "SEE LICENSE IN LICENSE",
  "requireRewrite": {
    "map": [
      [
        "#logger",
        "dist/src/setup/logger"
      ],
      [
        "#database",
        "dist/src/setup/database"
      ],
      [
        "#utils",
        "dist/src/utils"
      ],
      [
        "#services/",
        "dist/src/services/"
      ],
      [
        "#routes/",
        "dist/src/routes/"
      ],
      [
        "#models/",
        "dist/src/models/"
      ],
      [
        "#handlers/",
        "dist/src/handlers/"
      ],
      [
        "#daos/",
        "dist/src/database/"
      ],
      [
        "#src/",
        "dist/src/"
      ]
    ]
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "mocha --exit --timeout 10000 dist/tests",
    "start": "yarn run build && yarn run watch",
    "debug": "yarn run build && yarn run watch-debug",
    "build": "yarn run build-ts",
    "build-ts": "yarn run tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve\"",
    "watch-ts": "tsc -w",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"npm run serve-debug\"",
    "serve": "nodemon --delay 5 dist/src/index.js",
    "serve-debug": "nodemon --inspect dist/src/index.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "engines": {
    "node": ">=10.0",
    "yarn": ">=1.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/chai": "^4.1.4",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/knex": "^0.14.19",
    "@types/luxon": "^1.2.2",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.9.1",
    "@types/pg": "^7.4.10",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/uuid": "^3.4.3",
    "@types/xml2js": "^0.4.3",
    "chai": "^4.1.2",
    "concurrently": "^3.6.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/mime-types": "^2.1.0",
    "@types/multer": "^1.3.7",
    "@types/multer-s3": "^2.7.6",
    "@types/otplib": "^7.0.0",
    "@types/validator": "^9.4.3",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "cors": "^2.8.4",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "dotenv-safe": "^6.0.0",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.3",
    "express-validator": "^5.3.0",
    "fp-ts": "^1.7.1",
    "funfix": "^7.0.1",
    "io-ts": "^1.1.3",
    "io-ts-reporters": "^0.0.20",
    "knex": "^0.19.5",
    "luxon": "^1.3.3",
    "mime-types": "^2.1.19",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "multer-s3": "^2.7.0",
    "otplib": "^10.0.1",
    "pg": "^7.4.3",
    "purify-ts": "^0.11.0",
    "ramda": "^0.25.0",
    "require-rewrite": "^1.0.2",
    "source-map-support": "^0.5.6",
    "space-lift": "^0.8.0",
    "uuid": "^3.3.2",
    "validator": "^10.5.0",
    "winston": "^3.0.0",
    "xml2js": "^0.4.19"
  }
}
